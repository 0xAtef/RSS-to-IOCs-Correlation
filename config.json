  "feed_urls": [
    "https://www.us-cert.gov/ncas/alerts.xml",
    "https://www.cert.ssi.gouv.fr/feed/",
    "http://feeds.feedburner.com/TheHackersNews",
    "https://krebsonsecurity.com/feed/",
    "https://www.bleepingcomputer.com/feed/",
    "https://www.darkreading.com/rss.xml",
    "http://isc.sans.edu/rssfeed.xml",
    "https://newsroom.trendmicro.com/media-coverage?pagetemplate=rss",
    "https://feeds.feedburner.com/threatintelligence/pvexyqv7v0v",
    "https://darkwebinformer.com/rss/"
  ],

  "fixed_tags": ["RSS to IOC Collector"],

  "feed_tags_by_feed": {
    "www.us-cert.gov": ["us-cert"],
    "www.cert.ssi.gouv.fr": ["ssi-gouv"],
    "feeds.feedburner.com": ["TheHackersNews"],
    "krebsonsecurity.com": ["KrebsOnSecurity"],
    "www.bleepingcomputer.com": ["BleepingComputer"],
    "www.cisa.gov": ["CISA"],
    "www.darkreading.com": ["DarkReading"],
    "isc.sans.edu": ["SANS"],
    "newsroom.trendmicro.com": ["TrendMicro"],
    "feeds.feedburner.com/threatintelligence": ["ThreatIntelligence"],
    "darkwebinformer.com": ["DarkWebInformer"]
  },

  "whitelist_by_feed": {
    "*": ["x.ai", "x.com"],
    "darkwebinformer.com": [
      "portal.darkwebinformer.com",
      "identifiers.the",
      "agreements.sample",
      "sitetvtc.gov.sa"
    ],
    "isc.sans.edu": [
      "urlhistory.html",
      "sms-activate.org"
    ]
  },

  "ioc_context_keywords": {
    "c2": ["command and control", "c2 server", "beacon", "callback"],
    "malware": ["malware", "ransomware", "trojan"],
    "phishing": ["phishing", "spearphishing"]
  },

  "ioc_patterns": {
    "ips": "(?:\\\\d{1,3}\\\\.){3}\\\\d{1,3}",
    "domains": "\\\\b(?:[A-Za-z0-9-]+\\\\.)+[A-Za-z]{2,}\\\\b",
    "urls": "https?://[^\\\\s\\\"'>]+",
    "emails": "[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\\\.[A-Za-z]{2,}",
    "sha1": "\\\\b[a-fA-F0-9]{40}\\\\b"
  },

  "max_days_old": 20,
  "seen_iocs_path": "seen_iocs.json",
  "output_base_dir": "misp_feed",
  "log_file": "ioc_collector.log",
  "user_agent": "MISP-IOC-Collector/1.0",
  "request_timeout": 10,

  "http_retry_total": 3,
  "http_backoff_factor": 1,
  "http_status_forcelist": [429, 500, 502, 503, 504],
  "max_workers": 5,

  "misp_analysis": 0,
  "misp_threat_level_id": 4,
  "misp_tag_colour": "#004646"
}
